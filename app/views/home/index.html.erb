<h1>Welcome Home</h1>




<% if current_user && current_user.admin? %>
<%= link_to "View Profile", user_path(current_user) %>
  <% if @users.any? %>
    <% @users.each do |user| %>
      <%= link_to user.email, user_path(user)  %>
    <% end %>
  <% end %>
<% end %>

<% if @images.any? %>
  <% @images.each do |image| %>
    <%= link_to (image_tag image.file(:medium), :class => "hvr-grow image"), user_image_path(image.user, image) %>
  <% end %>
<% end %>
